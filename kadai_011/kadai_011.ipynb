        "#繰り返し処理を使いこなそう\n",
        "array = [\"水\",\"金\",\"地\",\"火\",\"木\",\"土\",\"天\",\"海\",\"冥\"]\n",
        "#forで表示\n",
        "for array_kadai in array:\n",
        "  print(array_kadai)\n",
        "#whileで表示\n",
        "i = 0\n",
        "while i < len(array):\n",
        "  print(array[i])\n",
        "  i += 1"
      ],
