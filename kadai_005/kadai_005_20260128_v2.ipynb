        "#課題：変数を使って四則演算しよう\n",
        "\n",
        "#上辺：10cm\n",
        "top_side = 10\n",
        "\n",
        "#下辺：20cm\n",
        "bottom_side = 20\n",
        "\n",
        "#高さ：5cm\n",
        "height = 5\n",
        "\n",
        "#台形の面積 =（上辺＋下辺）× 高さ ÷ 2\n",
        "menseki = (top_side + bottom_side) * height / 2\n",
        "\n",
        "#回答\n",
        "print(f\"{menseki}cm²\")"
      ],
