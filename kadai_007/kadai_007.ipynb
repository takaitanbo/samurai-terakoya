        "#リスト/ディクショナリを使いこなそう\n",
        "#リスト：array\n",
        "array = [\"月曜日は晴れです\",\"火曜日は雨です\",\"水曜日は晴れです\",\"木曜日は晴れです\",\"金曜日は曇りです\",\"土曜日は曇りのち雨です\",\"日曜日は雷雨です\"]\n",
        "#水曜日の値を表示\n",
        "print(array[2])\n",
        "\n",
        "#ディクショナリ\n",
        "dictionary = {\"mon\":\"晴れ\",\"tue\":\"雨\",\"wed\":\"晴れ\",\"thu\":\"晴れ\",\"fri\":\"雲\",\"sat\":\"曇りのち雨\",\"sun\":\"雷雨\"}\n",
        "#水曜日の値を表示\n",
        "print(dictionary[\"wed\"])\n"
